<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence
	http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd"
	version="2.0">
	<persistence-unit name="barPU" transaction-type="RESOURCE_LOCAL">
		<properties>
			<property name="hibernate.query.factory_class"
				value="org.hibernate.hql.internal.classic.ClassicQueryTranslatorFactory" />
			<property name="hibernate.dialect" value="org.hibernate.dialect.MySQL5Dialect" />
		</properties>
		<class>com.bar.dbaccess.entities.PaymentEntity</class>
		<class>com.bar.dbaccess.entities.DailyEntity</class>
		<class>com.bar.dbaccess.entities.IncomeEntity</class>
		<class>com.bar.dbaccess.entities.IncidentEntity</class>
		<class>com.bar.dbaccess.entities.UserEntity</class>
		<class>com.bar.dbaccess.entities.EntryMoneyEntity</class>		
	</persistence-unit>
</persistence>