<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
	xmlns='http://dozer.sourceforge.net'
	xsi:schemaLocation='http://dozer.sourceforge.net 
                   http://dozer.sourceforge.net/schema/beanmapping.xsd'>
	<mapping>
		<class-a>com.gu.admin.forms.Schedule</class-a>
		<class-b>com.gu.dbaccess.entities.ScheduleEntity</class-b>
		<field
			custom-converter="com.gu.converters.StringToDateConverter">
			<a>dateschedule</a>
			<b>dateschedule</b>
		</field>
	</mapping>
	<mapping>
		<class-a>com.gu.boss.forms.TPV</class-a>
		<class-b>com.gu.dbaccess.entities.TPVEntity</class-b>
		<field
			custom-converter="com.gu.converters.StringToDateConverter">
			<a>creationdate</a>
			<b>creationdate</b>
		</field>
	</mapping>
	<mapping>
		<class-a>com.gu.services.messages.Message</class-a>
		<class-b>com.gu.dbaccess.entities.MessageEntity</class-b>
		<field
			custom-converter="com.gu.converters.StringToTimeConverter">
			<a>datefrom</a>
			<b>datefrom</b>
		</field>
		<field
			custom-converter="com.gu.converters.StringToTimeConverter">
			<a>dateuntil</a>
			<b>dateuntil</b>
		</field>
		<field>
			<a>smessage</a>
			<b>message</b>
		</field>
	</mapping>
</mappings>